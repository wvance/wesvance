<p id="notice"><%= notice %></p>

<h1>Listing Foursquares</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>User</th>
      <th>Image</th>
      <th>Location</th>
      <th>Post</th>
      <th>Url</th>
      <th>Date</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foursquares.each do |foursquare| %>
      <tr>
        <td><%= foursquare.body %></td>
        <td><%= foursquare.user %></td>
        <td><%= foursquare.image %></td>
        <td><%= foursquare.location %></td>
        <td><%= foursquare.post_id %></td>
        <td><%= foursquare.url %></td>
        <td><%= foursquare.date %></td>
        <td><%= foursquare.latitude %></td>
        <td><%= foursquare.longitude %></td>
        <td><%= link_to 'Show', foursquare %></td>
        <td><%= link_to 'Edit', edit_foursquare_path(foursquare) %></td>
        <td><%= link_to 'Destroy', foursquare, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foursquare', new_foursquare_path %>
